{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "ie >= 9",
            "ff >= 30",
            "chrome >= 34",
            "safari >= 7",
            "opera >= 23",
            "bb >= 10"
          ]
        }
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "import",
      { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }
    ],
    "@babel/plugin-transform-runtime",
    // [
    //   "@babel/plugin-proposal-decorators",
    //   {
    //     "legacy": true
    //   }
    // ],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ],
  "comments": false
}
